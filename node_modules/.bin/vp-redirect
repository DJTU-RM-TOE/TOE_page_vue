#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/../.store/@vuepress+plugin-redirect@2.0.0-rc.8/node_modules/@vuepress"
else
  export NODE_PATH="$NODE_PATH:$basedir/../.store/@vuepress+plugin-redirect@2.0.0-rc.8/node_modules/@vuepress"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/@vuepress+plugin-redirect@2.0.0-rc.8/node_modules/@vuepress/plugin-redirect/lib/cli/index.js" "$@"
else
  exec node  "$basedir/../.store/@vuepress+plugin-redirect@2.0.0-rc.8/node_modules/@vuepress/plugin-redirect/lib/cli/index.js" "$@"
fi
